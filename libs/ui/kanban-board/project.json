{
  "projectType": "library",
  "root": "libs/ui/kanban-board",
  "sourceRoot": "libs/ui/kanban-board/src",
  "prefix": "budgetello",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/ui/kanban-board"
      ],
      "options": {
        "jestConfig": "libs/ui/kanban-board/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ui/kanban-board/src/**/*.ts",
          "libs/ui/kanban-board/src/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/angular",
        "port": 4400,
        "config": {
          "configFolder": "libs/ui/kanban-board/.storybook"
        },
        "projectBuildConfig": "ui-kanban-board:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "uiFramework": "@storybook/angular",
        "outputPath": "dist/storybook/ui-kanban-board",
        "config": {
          "configFolder": "libs/ui/kanban-board/.storybook"
        },
        "projectBuildConfig": "ui-kanban-board:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
