{
  "projectType": "library",
  "root": "libs/front-end/home/ui/add-board-form",
  "sourceRoot": "libs/front-end/home/ui/add-board-form/src",
  "prefix": "budgetello",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/front-end/home/ui/add-board-form"],
      "options": {
        "jestConfig": "libs/front-end/home/ui/add-board-form/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/front-end/home/ui/add-board-form/src/**/*.ts",
          "libs/front-end/home/ui/add-board-form/src/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/angular",
        "port": 4400,
        "config": {
          "configFolder": "libs/front-end/home/ui/add-board-form/.storybook"
        },
        "projectBuildConfig": "front-end-home-ui-add-board-form:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "uiFramework": "@storybook/angular",
        "outputPath": "dist/storybook/front-end-home-ui-add-board-form",
        "config": {
          "configFolder": "libs/front-end/home/ui/add-board-form/.storybook"
        },
        "projectBuildConfig": "front-end-home-ui-add-board-form:build-storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": ["type:ui", "domain:home", "app-level-domain:front-end"]
}
